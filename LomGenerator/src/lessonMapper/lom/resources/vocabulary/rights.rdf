      </rdf:Alt>
    </rdfs:comment>
  </rdf:Property>
  
  
 <rdf:Property rdf:ID="description">
    <rdfs:label> 
      <rdf:Alt>
	<rdf:li xml:lang="en">Description</rdf:li>
	<rdf:li xml:lang="es">Descripción</rdf:li>
      </rdf:Alt>
    </rdfs:label>
    <rdfs:comment>
      <rdf:Alt>
	<rdf:li xml:lang="en">Comments on the conditions of use of this learning object.</rdf:li>
      <rdf:li xml:lang="es">Comentarios sobre las condiciones de utilización de este objeto educativo</rdf:li>
    </rdf:Alt>
    </rdfs:comment>
  </rdf:Property>


</rdf:RDF>
