    </rdfs:label>
    <rdfs:comment>
      <rdf:Alt>
	<rdf:li xml:lang="en">Instances of this class represent costs.</rdf:li>
	<rdf:li xml:lang="es">Instancias de la clase representan costos.</rdf:li>
      </rdf:Alt>
    </rdfs:comment>
  </rdfs:Class>
  
  <lom_rights:Cost rdf:ID="SomeCost" lm:orderingValue="10">
