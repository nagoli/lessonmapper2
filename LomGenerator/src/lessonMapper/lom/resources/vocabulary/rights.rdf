      <rdf:Alt>
	<rdf:li xml:lang="en">Cost</rdf:li>
	<rdf:li xml:lang="es">Costo</rdf:li>
      </rdf:Alt>
    </rdfs:label>
    <rdfs:comment>
      <rdf:Alt>
	<rdf:li xml:lang="en">Whether use of this learning object requires payment.</rdf:li>
      <rdf:li xml:lang="es">Si el uso de este objeto de aprendizaje.</rdf:li>
    </rdf:Alt>
    </rdfs:comment>
  </rdf:Property>


  <rdfs:Class rdf:ID="CopyrightAndOtherRestrictions">
    <rdfs:label>
      <rdf:Alt>
	<rdf:li xml:lang="en">Copyright and other restrictions</rdf:li>
	<rdf:li xml:lang="es">Derecho y otra restricciones</rdf:li>
      </rdf:Alt>
    </rdfs:label>
    <rdfs:comment>
      <rdf:Alt>
	<rdf:li xml:lang="en">Instances of this class represent copyright and other restrictions.</rdf:li>
	<rdf:li xml:lang="es">Instancias de la clase representan derechos y otra </rdf:li>
      </rdf:Alt>
    </rdfs:comment>
  </rdfs:Class>
  
  <lom_rights:CopyrightAndOtherRestrictions rdf:ID="SomeRestriction" lm:orderingValue="30">
    <rdfs:label>  
      <rdf:Alt>
	<rdf:li xml:lang="en">Some Restriction</rdf:li>
	<rdf:li xml:lang="es">Unas Restriciones</rdf:li>
      </rdf:Alt>
    </rdfs:label>
  </lom_rights:CopyrightAndOtherRestrictions>
  <lom_rights:CopyrightAndOtherRestrictions rdf:ID="NoRestriction" lm:orderingValue="10">
    <rdfs:label>  
      <rdf:Alt>
	<rdf:li xml:lang="en">No Restriction</rdf:li>
	<rdf:li xml:lang="es">Sin Restriciones</rdf:li>
      </rdf:Alt>
    </rdfs:label>
