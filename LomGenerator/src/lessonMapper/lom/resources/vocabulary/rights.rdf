      <rdf:Alt>
	<rdf:li xml:lang="en">Some Cost</rdf:li>
	<rdf:li xml:lang="es">Tiene Costo</rdf:li>
      </rdf:Alt>
    </rdfs:label>
  </lom_rights:Cost>
  <lom_rights:Cost rdf:ID="NoCost">
    <rdfs:label>  
      <rdf:Alt>
	<rdf:li xml:lang="en">NoCost</rdf:li>
	<rdf:li xml:lang="es">Sin Costo</rdf:li>
      </rdf:Alt>
    </rdfs:label>
  </lom_rights:Cost>
  
  
  <rdf:Property rdf:ID="cost" lm:orderingValue="30">
